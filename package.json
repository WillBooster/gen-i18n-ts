{
  "name": "gen-i18n-ts",
  "version": "0.0.0-semantically-released",
  "repository": "git@github.com:WillBooster/gen-i18n-ts.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\" --color",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\" --color",
    "release": "semantic-release",
    "release-test": "yarn release -d",
    "start": "yarn && babel-node -x .js,.jsx,.es6,.es,.ts -- src/index.ts",
    "test": "yarn build && sh __tests__/test.sh",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "lodash": "4.17.20",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/helper-plugin-utils": "7.10.4",
    "@babel/node": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-numeric-separator": "7.12.7",
    "@babel/preset-env": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@rollup/plugin-babel": "5.2.2",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.0",
    "@types/builtin-modules": "^3",
    "@types/eslint": "7",
    "@types/jest": "26.0.19",
    "@types/lodash": "4.14.165",
    "@types/micromatch": "^4",
    "@types/node": "14.14.13",
    "@types/semantic-release": "^17",
    "@types/yargs": "15.0.11",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "@willbooster/eslint-config-ts": "5.1.1",
    "@willbooster/prettier-config": "5.1.0",
    "@willbooster/renovate-config": "5.1.1",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "builtin-modules": "3.1.0",
    "core-js": "3.8.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "micromatch": "4.0.2",
    "power-assert": "1.6.1",
    "prettier": "2.2.1",
    "rollup": "2.35.0",
    "rollup-plugin-node-externals": "2.2.0",
    "semantic-release": "17.3.0",
    "sort-package-json": "1.48.0",
    "typescript": "4.1.3"
  }
}
