{
  "name": "gen-i18n-ts",
  "version": "0.0.0-semantically-released",
  "description": "A CLI tool to generate a strictly-typed i18n object literal written in TypeScript from JSON files.",
  "keywords": [
    "TypeScript",
    "i18n",
    "internationalization"
  ],
  "repository": "git@github.com:WillBooster/gen-i18n-ts.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "release": "semantic-release",
    "release-test": "yarn release -d",
    "start": "yarn && babel-node -x .js,.jsx,.es6,.es,.ts -- src/start.ts",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "lodash": "4.17.21",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/helper-plugin-utils": "7.14.5",
    "@babel/node": "7.14.7",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-numeric-separator": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@types/builtin-modules": "3.1.1",
    "@types/eslint": "7.28.0",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.3.2",
    "@types/semantic-release": "17.2.1",
    "@types/yargs": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "@willbooster/eslint-config-ts": "6.3.0",
    "@willbooster/prettier-config": "6.2.1",
    "@willbooster/renovate-config": "6.3.0",
    "babel-jest": "27.0.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.1.0",
    "babel-preset-power-assert": "3.0.0",
    "builtin-modules": "3.2.0",
    "core-js": "3.15.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "2.23.4",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.0.1",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "prettier": "2.3.2",
    "rollup": "2.53.2",
    "rollup-plugin-node-externals": "2.2.0",
    "semantic-release": "17.4.4",
    "sort-package-json": "1.50.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=12"
  }
}
