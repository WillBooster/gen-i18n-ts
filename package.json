    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "yarn && babel-node -x .js,.jsx,.es6,.es,.ts -- src/index.ts",
    "test": "sh __tests__/test.sh",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-numeric-separator": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "26.0.10",
    "@types/node": "14.0.27",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.0",
    "@willbooster/eslint-config-ts": "4.0.3",
    "@willbooster/prettier-config": "3.0.0",
