    "babel-preset-power-assert": "3.0.0",
    "core-js": "3.6.5",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "lint-staged": "10.2.11",
    "power-assert": "1.6.1",
    "prettier": "2.0.5",
    "sort-package-json": "1.44.0",
    "ts-jest": "26.2.0",
    "typescript": "3.9.7"
  }
