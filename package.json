{
  "name": "gen-i18n-ts",
  "version": "0.0.0-semantically-released",
  "description": "A CLI tool to generate a strictly-typed i18n object literal written in TypeScript from JSON files.",
  "keywords": [
    "TypeScript",
    "i18n",
    "internationalization"
  ],
  "repository": "git@github.com:WillBooster/gen-i18n-ts.git",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__,samples}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepublishOnly": "pinst --disable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "postpublish": "pinst --enable",
    "start": "yarn && babel-node -x .js,.jsx,.es6,.es,.ts -- src/start.ts",
    "start-prod": "yarn && yarn build && yarn gen-i18n-ts",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "js-yaml": "4.1.0",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/helper-plugin-utils": "7.14.5",
    "@babel/node": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-numeric-separator": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/builtin-modules": "3.1.1",
    "@types/eslint": "7.29.0",
    "@types/jest": "27.0.3",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.10",
    "@types/semantic-release": "17.2.3",
    "@types/yargs": "17.0.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@willbooster/eslint-config-ts": "6.4.2",
    "@willbooster/prettier-config": "6.4.0",
    "@willbooster/renovate-config": "6.4.2",
    "babel-jest": "27.3.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-power-assert": "3.0.0",
    "builtin-modules": "3.2.0",
    "core-js": "3.19.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "micromatch": "4.0.4",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "prettier": "2.5.0",
    "rollup": "2.60.1",
    "rollup-plugin-node-externals": "2.2.0",
    "semantic-release": "18.0.1",
    "sort-package-json": "1.53.1",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": ">=12"
  },
  "packageManager": "yarn@3.2.0-rc.6"
}
