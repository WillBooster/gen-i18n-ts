{
  "name": "gen-i18n-ts",
  "version": "0.0.0-semantically-released",
  "description": "A CLI tool to generate a strictly-typed i18n object literal written in TypeScript from JSON files.",
  "keywords": [
    "TypeScript",
    "i18n",
    "internationalization"
  ],
  "repository": "github:WillBooster/gen-i18n-ts",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "babel-node -x .js,.jsx,.es6,.es,.ts -- src/start.ts",
    "start-prod": "yarn build && yarn gen-i18n-ts",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "dependencies": {
    "js-yaml": "4.1.0",
    "yargs": "17.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/helper-plugin-utils": "7.18.9",
    "@babel/node": "7.18.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@types/builtin-modules": "3.1.1",
    "@types/eslint": "8.4.6",
    "@types/jest": "28.1.7",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.50",
    "@types/semantic-release": "17.2.4",
    "@types/yargs": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@willbooster/eslint-config-ts": "9.0.0",
    "@willbooster/prettier-config": "9.0.0",
    "@willbooster/renovate-config": "9.3.0",
    "babel-jest": "28.1.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-power-assert": "3.0.0",
    "builtin-modules": "3.3.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "core-js": "3.24.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "power-assert": "1.6.1",
    "prettier": "2.7.1",
    "rollup": "2.78.1",
    "rollup-plugin-node-externals": "4.1.1",
    "semantic-release": "19.0.3",
    "sort-package-json": "1.57.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@4.0.0-rc.14",
  "engines": {
    "node": ">=14"
  }
}
