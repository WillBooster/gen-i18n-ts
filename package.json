{
  "name": "gen-i18n-ts",
  "version": "0.0.0-semantically-released",
  "description": "A CLI tool to generate a strictly-typed i18n object literal written in TypeScript from JSON files.",
  "keywords": [
    "TypeScript",
    "i18n",
    "internationalization"
  ],
  "repository": "github:WillBooster/gen-i18n-ts",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "bin": "./bin/index.cjs",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{__tests__,scripts,src}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "babel-node -x .js,.jsx,.es6,.es,.ts -- src/start.ts",
    "start-prod": "yarn build && yarn gen-i18n-ts",
    "test": "yarn jest",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "dependencies": {
    "js-yaml": "4.1.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/helper-plugin-utils": "7.20.2",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "23.0.7",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/builtin-modules": "3.1.1",
    "@types/eslint": "8.4.10",
    "@types/jest": "29.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.18",
    "@types/semantic-release": "17.2.5",
    "@types/yargs": "17.0.20",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@willbooster/babel-configs": "1.3.0",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "babel-jest": "29.4.1",
    "babel-plugin-transform-remove-console": "6.9.4",
    "builtin-modules": "3.3.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "core-js": "3.27.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "lint-staged": "13.1.0",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.3",
    "rollup": "3.11.0",
    "rollup-plugin-node-externals": "5.1.0",
    "semantic-release": "19.0.5",
    "sort-package-json": "2.2.0",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@4.0.0-rc.36",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
