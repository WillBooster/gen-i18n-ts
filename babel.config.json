{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.8",
        "bugfixes": true
      }
    ],
    "@babel/typescript",
    "power-assert"
  ],
  "plugins": ["@babel/proposal-class-properties", "@babel/proposal-numeric-separator"],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-remove-console",
          {
            "exclude": ["error", "info", "warn"]
          }
        ],
        "babel-plugin-unassert"
      ]
    }
  }
}
