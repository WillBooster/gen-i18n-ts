{
  "name": "@willbooster/babel-configs",
  "version": "1.3.1",
  "description": "A set of babel configs for WillBooster projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "files": [
    "babel.*.cjs"
  ],
  "scripts": {
    "cleanup": "yarn format",
    "format": "sort-package-json && yarn prettify",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\""
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/babel__core": "7.20.3",
    "@willbooster/prettier-config": "9.1.2",
    "core-js": "3.33.1",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "sort-package-json": "2.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
